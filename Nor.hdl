// using Nor for logical purposes, same chip implimented in p1
/**
 * Nor gate: 
 * out = 1 if (a == 0 and b == 0)
 *       0 otherwise
 */

CHIP Nor {
    IN a, b;
    OUT out;
	
    PARTS:

	
	Nand(a=a,b=a,out=NotA);
	Nand(a=b,b=b,out=NotB);
	Nand(a=NotA,b=NotB,out=outAorB);
	Nand(a=outAorB,b=outAorB,out=out);
}
